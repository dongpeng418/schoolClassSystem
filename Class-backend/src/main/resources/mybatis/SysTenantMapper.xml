    from sys_tenant
  </select>
  
  <select id="selectByDomain" resultMap="BaseResultMap" parameterType="cn.com.school.classinfo.model.SysTenant" >
    select 
    <include refid="Base_Column_List" />
    from sys_tenant
     <where>
      <if test="domain != null" >
        and domain = #{domain,jdbcType=VARCHAR}
      </if>
    </where>
  </select>
